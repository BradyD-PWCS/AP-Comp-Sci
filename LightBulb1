package Light;

public class LightBulb1 {
		private boolean isOn;
		private int wattage;
		private String color;
		
		public LightBulb1(int w, boolean i, String c) {
			wattage = w;
			isOn = i;
			color = c;
		}

		public void turnOff() {
			isOn = false;
			
		}
		public void turnOn() {
			isOn = true;
		}


}
